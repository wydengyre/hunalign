{
  "importMap": "import_map.json",
  "tasks": {
    "bundle": "mkdir -p dist/deno && deno bundle deno/hunalign.ts dist/deno/hunalign.js",
    "clean": "rm -rf dist/deno",
    "fmt": "deno fmt deno scripts ts web",
    "lint": "deno lint deno ts",
    "test": "deno test --allow-read=. deno",
    "test_web": "deno test --allow-net --allow-read --allow-write --allow-env --allow-run web/end-to-end.test.ts"
  }
}
